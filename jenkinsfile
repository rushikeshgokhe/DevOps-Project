pipeline {
    agent any

    stages {
        stage('Checkout Code') {
            steps {
                git 'https://github.com/rushikeshgokhe/DevOps-Project.git'
            }
        }

        stage('Build') {
            steps {
                echo 'Build completed successfully'
            }
        }

        stage('Deploy to S3') {
            steps {
                sh 'bash scripts/deploy.sh'
            }
        }
    }
}
